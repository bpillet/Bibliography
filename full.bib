@Article{AmerikVerbitsky_Automorphisms,
  Title                    = {Construction of automorphisms of hyperkähler manifolds},
  Author                   = {Amerik, Ekaterina and Verbitsky, Misha},
  Journal                  = {arXiv:1604.03079 [math]},
  Year                     = {2016},

  Month                    = apr,
  Note                     = {arXiv: 1604.03079},

  Abstract                 = {Let $M$ be an irreducible holomorphic symplectic (hyperkähler) manifold. If $b\_2(M) \geq 5$, we construct a deformation $M'$ of $M$ which admits a symplectic automorphism of infinite order. This automorphism is hyperbolic, that is, its action on the space of real $(1,1)$-classes is hyperbolic. If $b_2(M) \geq 14$, similarly, we construct a deformation which admits a parabolic automorphism.},
  Annote                   = {Comment: 16 pages, LaTeX},
  File                     = {arXiv\:1604.03079 PDF:/home/basile/.mozilla/firefox/otpffccs.default/zotero/storage/ET5D7IUM/Amerik et Verbitsky - 2016 - Construction of automorphisms of hyperkahler man.pdf:application/pdf;arXiv.org Snapshot:/home/basile/.mozilla/firefox/otpffccs.default/zotero/storage/XAGQN4KK/1604.html:text/html},
  Keywords                 = {14J50, 53C26 (primary), 11E12 (secondary), Mathematics - Algebraic Geometry},
  Url                      = {http://arxiv.org/abs/1604.03079},
  Urldate                  = {2017-01-08}
}

